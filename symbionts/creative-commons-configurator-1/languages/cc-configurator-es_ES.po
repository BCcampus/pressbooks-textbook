msgid "Blog Name"
msgstr "Nombre del Blog"

#: bccl-utils.php:71
msgid "First + Last Name"
msgstr "Nombre + Apellido"

#: bccl-utils.php:72
msgid "Last + First Name"
msgstr "Apellido + Nombre"

#: bccl-utils.php:73
